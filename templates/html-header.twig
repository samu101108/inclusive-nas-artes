    <style>
        @media (min-width: 1200px) {
            .container {
                max-width: 1254px;
                margin: 0 auto;
            } 
        }
        blockquote {position: relative;}
        blockquote::before { 
            content: open-quote;
            position: absolute;
            top: -30px;
            left: -34px;
            font-size: 80px;
            color: #BD887150;
        }
        blockquote::after { 
            content: close-quote;
            position: absolute;
            bottom: -80px;
            right: -12px;
            font-size: 80px;
            color: #BD887150;
        }

    </style>


